{
  "name": "tourze/server-terminal-bundle",
  "description": "服务器终端模拟",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/doctrine-bridge": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/serializer": "^6.4",
    "tourze/arrayable": "0.0.*",
    "tourze/biz-user-bundle": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/doctrine-track-bundle": "0.1.*",
    "tourze/easy-admin-attribute": "0.1.*",
    "tourze/enum-extra": "~0.0.5"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "ServerTerminalBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ServerTerminalBundle\\Tests\\": "tests"
    }
  }
}